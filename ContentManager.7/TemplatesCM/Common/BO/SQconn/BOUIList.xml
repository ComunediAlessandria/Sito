<?xml version='1.0' encoding='iso-8859-1' ?>
<uilist id='SimpleQueryConf'>

	<page
		title='__(bo-sq.listtitle)'
		permission='SQ'
	/>

	<presenter
		type='Template'
		filename='SQconn/tList.html'
	/>

	<options>
		<option name='modlink' label='MODIFICA configurazione %s' element='ConnectionName' />
		<option name='dellink' label='CANCELLA configurazione %s' element='ConnectionName' />
	</options>

	<elements>
		<element	id='ConnectionName'		bo='ConnectionName'		label='ConnectionName' 	/>
		<element	id='DBHost'				bo='DBHost'		label='DBHost' 	/>
		<element	id='DBUser'				bo='DBUser'		label='DBUser' 	/>
		<element	id='DBDriver'				bo='DBDriver'		label='DBDriver' 	/>
		<element	id='DBName'				bo='DBName'		label='DBName' 	/>
		<element	id='ConnectionString'	bo='ConnectionString'		label='ConnectionString' 	/>
	</elements>

	<sortOrder>
		<sortKey	when='after'	bo='ConnectionName'	mode='n'	order='DESC' />
	</sortOrder>


</uilist>
