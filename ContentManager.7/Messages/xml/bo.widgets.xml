		/>

		<message id='ui-browser'
			lang:it='%s versione %s'
			lang:en='%s version %s'
		/>

		<message id='ui-bs'>
			<trans lang='it'>SUPPORTATO</trans>
			<trans lang='en'>SUPPORTED</trans>
		</message>

		<message id='ui-bns'>
			<trans lang='it'>NON SUPPORTATO</trans>
