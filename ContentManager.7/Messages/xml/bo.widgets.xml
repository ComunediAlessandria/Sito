		</message>

		<message id='ui-w'
			lang:it='Benvenuto'
			lang:en='Welcome'
		/>

		<message id='ui-mg'
			lang:it='Gruppo Principale'
			lang:en='Main Group'
		/>

		<message id='ui-la'
			lang:it='Ultimo accesso'
			lang:en='Last login'
		/>

		<message id='ui-bl'
			lang:it='Lingua backoffice'
			lang:en='Backoffice Language'
		/>

		<message id='ui-bv'
			lang:it='Versione Browser'
			lang:en='Browser Version'
		/>

		<message id='ui-bp'
