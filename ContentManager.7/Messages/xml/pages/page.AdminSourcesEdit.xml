<?xml version='1.0' encoding='utf-8' ?>
<!DOCTYPE FlexCMPMessages SYSTEM '/FlexCMPMessages.dtd'>
<messages xmlns:lang='#' defaultlang='IT'>
	<group id='pgase' define='false'>

		<message id='pt'
			lang:it='Modifica Sorgente'
			lang:en='Source'
		/>

		<message id='nof'
			lang:it='Nessun file inserito'
			lang:en='No file'
		/>

		<!-- validazioni -->

		<message id='vcsv'
			lang:it='È necessario specificare il nome di un archivio CSV'
			lang:en='No CSV file selected'
		/>

		<message id='vnm'
			lang:it='È necessario specificare il nome della sorgente'
			lang:en='No source name specified'
		/>

		<!-- riepilogo -->

		<message id='rrows'
			lang:it='(Sono visualizzate solamente le prime %s righe)'
			lang:en='(Only the first %s rows are showed)'
		/>

		<message id='rsum'
			lang:it='Il documento è composto da %s nominativi. Il numero di colonne è pari a %s. Il numero di righe che non hanno il corretto numero di colonne è %s.'
			lang:en='The document is made of %s names. The number of columns is %s. Rows having the wrong columns number are %s.'
		/>

		<!-- template (inline) -->

		<message id='ln'
			lang:it='Nome'
			lang:en='Name'
		/>

		<message id='lk'
			lang:it='Tipo'
			lang:en='Kind'
		/>

		<message id='lc'
			lang:it='Documento CSV'
			lang:en='CSV Document'
		/>

		<message id='bv'
			lang:it='Verifica la correttezza del file'
			lang:en='Check correctness of the file'
		/>

		<message id='bok'
			lang:it='Registra le Modifiche'
			lang:en='Save'
		/>

		<message id='bcan'
			lang:it='Annulla le Modifiche'
			lang:en='Cancel'
		/>
		
	
		<message id='errnoemailcolumn'>
			<trans lang='it'><![CDATA[
				<strong>Errore</strong>: La sorgente csv non contiene la colonna obbligatoria chiamata email
			]]></trans>
			<trans lang='en'><![CDATA[
				<strong>Error</strong>: The source csv file does not contain the required column called email
			]]></trans>
		</message>

		

	</group>
</messages>